{
  "name": "shop-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run sass:watch\" \"vite\"",
    "build": "npm run sass:build && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "sass:watch": "sass --watch src/styles/scss/global.scss:src/styles/dist/global.css --style=expanded",
    "sass:build": "sass src/styles/scss/global.scss:src/styles/dist/global.css --style=compressed"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sass": "^1.93.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vite": "^7.1.7"
  }
}
